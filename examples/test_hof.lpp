// Test Higher-Order Functions
fn double(x: int) -> int {
    return x * 2;
}

fn isEven(x: int) -> bool {
    return x % 2 == 0;
}

fn add(a: int, b: int) -> int {
    return a + b;
}

fn main() -> int {
    let numbers = [1, 2, 3, 4, 5];
    
    // Test map
    let doubled = map(numbers, double);
    print("Doubled:");
    
    // Test filter  
    let evens = filter(numbers, isEven);
    print("Evens:");
    
    // Test reduce
    let sum = reduce(numbers, 0, add);
    print("Sum:");
    print(sum);
    
    return 0;
}
