// Test Composition - Direct Usage
fn double(x: int) -> int {
    return x * 2;
}

fn triple(x: int) -> int {
    return x * 3;
}

fn main() -> int {
    // Invece di composizione, usiamo chiamate annidate
    // che sono equivalenti: f(g(h(x)))
    let x = 5;
    let result = double(triple(x));  // double(triple(5)) = double(15) = 30
    
    print("Double of triple of 5:");
    print(result);
    
    return 0;
}
