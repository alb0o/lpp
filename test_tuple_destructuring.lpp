// Test Tuple Destructuring in L++
#pragma paradigm hybrid

fn main() -> int {
    // Create tuple
    let point = (10, 20, 30);
    
    // Destructure tuple
    let (x, y, z) = point;
    
    print("X:");
    print(x);
    print("Y:");
    print(y);
    print("Z:");
    print(z);
    
    // Destructure directly from tuple literal
    let (name, age, active) = ("Alice", 30, true);
    
    print("Name:");
    print(name);
    print("Age:");
    print(age);
    
    // Nested tuple destructuring
    let pair = ((1, 2), (3, 4));
    let (first, second) = pair;
    
    print("Tuple destructuring works!");
    
    return 0;
}
