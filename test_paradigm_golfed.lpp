#pragma paradigm golfed

// GOLFED MODE TEST - Ultra-minimal syntax

// Short function names (1-3 chars recommended)
fn f(x: int) -> int { <- x * 2; }
fn g(a: int, b: int) -> int { <- a + b; }

// Short variable names
fn m(n: int) -> int {
    let s: int = 0;
    let i: int = 1;
    while (i <= n) {
        s += i;
        i++;
    }
    <- s;
}

// Implicit return
fn d(x: int) -> int { x * 2; }
